module sys.types
{
// Some standard types

// An ISO-8601 date in ASCII 
typedef string date;

// An ISO-8601 datetime in ASSCI
typedef string datetime;

struct pair<T1,T2>
{
    T1 v1;
    T2 v2;
};

union either<T1,T2>
{
    T1 left;
    T2 right;
};

union maybe<T>
{
    void nothing;
    T just;
};

union error<T>
{
    T value;
    string error;
};

typedef vector<pair<K,V>> map<K,V>;

typedef vector<T> set<T>;

};
