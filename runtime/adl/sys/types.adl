module sys.types
{
// Some standard types

struct Pair<T1,T2>
{
    T1 v1;
    T2 v2;
};

union Either<T1,T2>
{
    T1 left;
    T2 right;
};

union Maybe<T>
{
    Void nothing;
    T just;
};

union Error<T>
{
    T value;
    String error;
};

typedef Vector<Pair<K,V>> Map<K,V>;

typedef Vector<T> Set<T>;

};
