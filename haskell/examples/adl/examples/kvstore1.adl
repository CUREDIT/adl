module examples.kvstore1
{

import sys.types.*;
import sys.rpc.*;

type Key = String;
type Value = String;
type Pattern = String;
type KVPair = Pair<Key,Value>;
type QueryResults = Vector<KVPair>;

union KVRequest
{
    Rpc<KVPair,Void> put;
    Rpc<Key,Void> delete;
    Rpc<Pattern,QueryResults> query;
};

type KVService = Sink<KVRequest>;

};
