module examples.kvstore1
{

import sys.types.*;
import sys.rpc.*;

typedef String Key;
typedef String Value;
typedef String Pattern;
typedef Pair<Key,Value> KVPair;
typedef Vector<KVPair> QueryResults;

union KVRequest
{
    Rpc<KVPair,Void> put;
    Rpc<Key,Void> delete;
    Rpc<Pattern,QueryResults> query;
};

typedef Sink<KVRequest> KVService;

};
